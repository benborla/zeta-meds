<!DOCTYPE html>
<html lang="{{ app.request.locale|slice(0, 2) }}">
<head>
    <title>{% block title %}Zeta Meds{% endblock %}</title>
	<meta charset="UTF-8">
	<meta name="description" content=" Divisima | eCommerce Template">
	<meta name="keywords" content="divisima, eCommerce, creative, html">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<!-- Favicon -->
    <link href="{{ asset('zetameds-theme/images/favicon.ico', 'zetaMedsTheme') }}" rel="shortcut icon"/>

	<!-- Google Font -->
	<link href="https://fonts.googleapis.com/css?family=Raleway:300,300i,400,400i,700,700i" rel="stylesheet">


	<!-- Stylesheets -->
    {% block stylesheets %}
        {{ encore_entry_link_tags('app', null, 'zetaMedsTheme') }}

        {{ sonata_block_render_event('sylius.shop.layout.stylesheets') }}
    {% endblock %}

    {{ sonata_block_render_event('sylius.shop.layout.head') }}


	<!--[if lt IE 9]>
		  <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
	  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
	<![endif]-->

</head>
<body>
	<!-- Page Preloder -->
	<div id="preloder">
		<div class="loader"></div>
	</div>

	<!-- Header section -->
	<header class="header-section">
		<div class="header-top">
			<div class="container">
				<div class="row">
					<div class="col-lg-2 text-center text-lg-left">
						<!-- logo -->
						<a href="./index.html" class="site-logo">
                            <img src="{{ asset('zetameds-theme/images/logo.png', 'zetaMedsTheme') }}" alt="logo">
						</a>
					</div>
					<div class="col-xl-6 col-lg-5">
						<form class="header-search-form">
							<input type="text" placeholder="Search on divisima ....">
							<button><i class="flaticon-search"></i></button>
						</form>
					</div>
					<div class="col-xl-4 col-lg-5">
						<div class="user-panel">
							<div class="up-item">
								<i class="flaticon-profile"></i>
								<a href="#">Sign</a> In or <a href="#">Create Account</a>
							</div>
							<div class="up-item">
								<div class="shopping-card">
									<i class="flaticon-bag"></i>
									<span>0</span>
								</div>
								<a href="#">Shopping Cart</a>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<nav class="main-navbar">
			<div class="container">
				<!-- menu -->
				<ul class="main-menu">
					<li><a href="#">Home</a></li>
					<li><a href="#">Women</a></li>
					<li><a href="#">Men</a></li>
					<li><a href="#">Jewelry
						<span class="new">New</span>
					</a></li>
					<li><a href="#">Shoes</a>
						<ul class="sub-menu">
							<li><a href="#">Sneakers</a></li>
							<li><a href="#">Sandals</a></li>
							<li><a href="#">Formal Shoes</a></li>
							<li><a href="#">Boots</a></li>
							<li><a href="#">Flip Flops</a></li>
						</ul>
					</li>
					<li><a href="#">Pages</a>
						<ul class="sub-menu">
							<li><a href="./product.html">Product Page</a></li>
							<li><a href="./category.html">Category Page</a></li>
							<li><a href="./cart.html">Cart Page</a></li>
							<li><a href="./checkout.html">Checkout Page</a></li>
							<li><a href="./contact.html">Contact Page</a></li>
						</ul>
					</li>
					<li><a href="#">Blog</a></li>
				</ul>
			</div>
		</nav>
	</header>
	<!-- Header section end -->



	<!-- Hero section -->
	<section class="hero-section">
		<div class="hero-slider owl-carousel">
			<div class="hs-item set-bg" data-setbg="{{ asset('zetameds-theme/images/slider-1.png', 'zetaMedsTheme') }}">
				<div class="container">
					<div class="row">
						<div class="col-xl-6 col-lg-7 text-white">
							<h2 class="drop-shadow">Zeta Meds</h2>
							<p class="drop-shadow">Zeta is an American company and a licensed pharmacy dedicated to providing its customers with a service unmatched by our competitors.</p>
						</div>
					</div>
				</div>
			</div>
			<div class="hs-item set-bg" data-setbg="{{ asset('zetameds-theme/images/slider-2.png', 'zetaMedsTheme') }}">
				<div class="container">
					<div class="row">
						<div class="col-xl-6 col-lg-7 text-white">
							<h2 class="drop-shadow">Zeta Meds</h2>
							<p class="drop-shadow">We are dedicated to being consistently reliable while having only the highest quality FDA-approved products available. With the friendliest customer service, lowest rates, and superb product line, Zeta is here to be the last place you ever have to look for the remedies you need.</p>
						</div>
					</div>
				</div>
			</div>
		</div>
		<div class="container">
			<div class="slide-num-holder" id="snh-1"></div>
		</div>
	</section>
	<!-- Hero section end -->



	<!-- Features section -->
	<section class="features-section">
		<div class="container-fluid">
			<div class="row">
				<div class="col-md-4 p-0 feature">
					<div class="feature-inner">
						<div class="feature-icon">
                            <img src="{{ asset('zetameds-theme/images/pencil-square-o.png', 'zetaMedsTheme') }}" alt="/img/icons/1.png">
						</div>
						<h2>Fill Out a Form</h2>
					</div>
				</div>
				<div class="col-md-4 p-0 feature">
					<div class="feature-inner">
						<div class="feature-icon">
                            <img src="{{ asset('zetameds-theme/images/credit-card.png', 'zetaMedsTheme') }}" alt="/img/icons/1.png">
						</div>
						<h2>Make the Payment</h2>
					</div>
				</div>
				<div class="col-md-4 p-0 feature">
					<div class="feature-inner">
						<div class="feature-icon">
                            <img src="{{ asset('zetameds-theme/images/home.png', 'zetaMedsTheme') }}" alt="/img/icons/3.png">
						</div>
						<h2>Receive your Package</h2>
					</div>
				</div>
			</div>
		</div>
	</section>
	<!-- Features section end -->

	<section class="top-letest-product-section">
		<div class="container">
            {% include '@SyliusShop/_flashes.html.twig' %}

            {{ sonata_block_render_event('sylius.shop.layout.before_content') }}
            {% block content %}
            {% endblock %}

            {{ sonata_block_render_event('sylius.shop.layout.after_content') }}
		</div>
	</section>

	<!-- Footer section -->
    {% block footer %}
        {% include '@SyliusShop/_footer.html.twig' %}
    {% endblock %}

	<!--====== Javascripts & Jquery ======-->
{% block javascripts %}
    {{ encore_entry_script_tags('app', null, 'zetaMedsTheme') }}

    {{ sonata_block_render_event('sylius.shop.layout.javascripts') }}
{% endblock %}

{{ sonata_block_render_event('sylius.shop.layout.after_body') }}

	</body>
</html>
